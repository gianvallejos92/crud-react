.FormWrapper
  padding: 0px 0px

.FormItem
  padding: 10px 5px
  display: flex
  .FormTitle
    width: 125px;
    font-size: 0.9rem;
    padding: 0px 5px;
    font-weight: bold;
    display: flex;
    justify-content: left;
    align-items: center;
  .FormInput
    width: 100%
  .FormControl
    width: 100%
    height: 34px
    line-height: 1.42
    border-radius: 4px
    border: 1px solid #ccc
    padding: 0px 10px